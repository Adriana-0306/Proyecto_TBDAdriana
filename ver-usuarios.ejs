<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <link rel="stylesheet" href="/styles.css">
  <title>Usuarios</title>
</head>
<body>
  <%- include('partials/navbar') %>

  <h1>Usuarios</h1>
  <table>
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Tipo de usuario</th>
      </tr>
    </thead>
    <tbody>
      <% usuarios.forEach(usuario => { %>
        <tr>
          <td><%= usuario.nombre_usuario %></td>
          <td><%= usuario.tipo_usuario %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>

  
</body>
</html>
